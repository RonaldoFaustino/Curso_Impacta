
public class ExemploLiterais {

	public static void main(String[] args) {
		
		int idade = 010;
		double salario = 3523.18;
		double tamanhoMoleculo = 2.34e10;
		boolean fumante = false;
		char sexo ='M';
		
		double distancia = 389.56;
		float num = (float) distancia;
		
		System.out.println(idade);
		System.out.println(salario);
		System.out.println(tamanhoMoleculo);
		System.out.println(fumante);
		System.out.println(sexo);
		System.out.println(num);

	}

}
