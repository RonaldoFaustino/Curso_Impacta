
public class OperadoresDecrementoIncremento {
	public static void main(String[] args) {
		
		/******** Primeiro exemplo ***********
		
		int a = 12;
		
		a++;
		a--; */
		
		int a , b, c;
		a = 10;
		
		b = a++;
		c = ++a;
		System.out.println(a);
		System.out.println(b);
		System.out.println(b);
	}
}
